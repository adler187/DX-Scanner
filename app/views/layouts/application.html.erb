<!DOCTYPE html>
<html>
<head>
  <title><%= yield(:title) %></title>
  <%= stylesheet_link_tag 'scaffold' %>
  <%= stylesheet_link_tag 'application' %>
  <%= javascript_include_tag 'jquery-min' %>
  <%= javascript_include_tag 'application' %>
    
  <%= yield(:head) %>
  <%= csrf_meta_tag %>
</head>
<body>
<% flash.each do |key, msg| %>
  <%= content_tag :div, msg, :id => key %>
<% end %>
 
<h1><%= yield :title %></h1>
<%= yield %>

</body>
</html>
