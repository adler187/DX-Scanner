<% title 'Logs' %>

<%= paginate @logs %>

<table>
  <tr>
    <th>Signal strength</th>
    <th>Signal to noise</th>
    <th>Signal quality</th>
    <th>Station</th>
    <th>Tuner</th>
  </tr>

<% @logs.each do |log| %>
  <tr>
    <td><%=h log.signal_strength %></td>
    <td><%=h log.signal_to_noise %></td>
    <td><%=h log.signal_quality %></td>
    <td><%=h log.station.callsign %></td>
    <td><%=h log.tuner.name %></td>
    <td><%= link_to 'Show', log %></td>
    <td><%= link_to 'Edit', edit_log_path(log) %></td>
    <td><%= link_to 'Destroy', log, :data => { :confirm => 'Are you sure?' }, :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New log', new_log_path %>